ip 192.168.10.2/3
gat 192.168.10.1

ip 192.168.20.2/3
gat 192.168.20.1

r1
enable
configure terminal
interface g0/0
ip address 192.168.10.1 255.255.255.0
no shutdown
interface s0/0
ip address 10.0.0.1 255.255.255.252
clock rate 64000
no shutdown
exit
ip route 192.168.20.0 255.255.255.0 10.0.0.2

r2
enable
configure terminal
interface g0/0
ip address 192.168.20.1 255.255.255.0
no shutdown
interface s0/0
ip address 10.0.0.2 255.255.255.252
no shutdown
exit
ip route 192.168.10.0 255.255.255.0 10.0.0.1

r1,r2
show ip route




